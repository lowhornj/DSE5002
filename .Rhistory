fib  <- function(n)
{
if (length(n) > 1) return(sapply(n, fib))
if (n == 1) return(1)
if (n == 2) return(1)
return(fib(n-1)+fib(n-2))
}
catalan <- function(n) {
cat <- vector("numeric", length(max(n)) + 1)
for (i in 0:max(n)) {
if (i == 0) {
cat[i+1] <- 1
} else {
cat[i+1] <- ((4*i - 2)*cat[i])/(i + 1)
}
}
return(cat[n + 1])
}
